generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db?connection_limit=1"
}

model erneuerbareElektrizitatsproduktionNachEnergietragernUndGemeinden {
  nr_gemeinde String
  biogasanlagen_abwasser Float?
  biogasanlagen_industrie Float?
  biogasanlagen_landwirtschaft Float?
  biomasse_holz Float?
  einwohner Int?
  gemeinde_name String
  jahr String
  kehricht Float?
  photovoltaik Float?
  wasserkraft Float?
  wind Float?
  total Float?

  @@id([nr_gemeinde, jahr])
}

model polizeiposten {
    key String @unique
    art String
    koordinaten String
}

model schutzraeume{
    key String @unique
    plaetze String
    bau String
    koordinaten String
}

model notfalltreffpunkte{
    key String @unique
    koordinaten String
}

model quellfassungen{
    key String @unique
    min String?
    max String?
    koordinaten String
}

model strom{
    key String @unique
    koordinaten_liste String
}

model Risikostelle{
    key String @unique
    koordinaten String
}

model Strassenverkehrzaehlung{
    key Int @id @default(autoincrement())
    jahr String
    messort String
    fahrzeugeProTag Int
    koordinaten String
}

model kreisverkehr{
    key String @unique
    name String
    besitzer String
    koordinaten String
}
