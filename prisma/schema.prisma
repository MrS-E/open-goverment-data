generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model energy {
  id String @id @default(cuid())
  nr_gemeinde String
  biogasanlagen_abwasser Float
  biogasanlagen_industrie Float
  biogasanlagen_landwirtschaft Float
  biomasse_holz Float
  einwohner Float
  gemeinde_name String
  jahr String
  kehricht Float
  photovoltaik Float
  wasserkraft Float
  wind Float
  total Float
}